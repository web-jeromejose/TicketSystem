@model TicketSystem.ViewModel.AccountIdentity_CreateViewModel

@{
    ViewData["Title"] = "Create";
}


    <div class="row">
        <div class="col s4">

            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="red-text"></div>
                <div class="form-group">
                    <label asp-for="Email" class="control-label"></label>
                    <input asp-for="Email" class="form-control" />
                    <span asp-validation-for="Email" class="red-text"></span>
                </div>
                <div class="form-group">
                    <label class="control-label">Confirm Email</label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="red-text"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Password" class="control-label"></label>
                    <input asp-for="Password" class="form-control" />
                    <span asp-validation-for="Password" class="red-text "></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

